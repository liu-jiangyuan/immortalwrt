board_config_update

case $board in
bt,r320)
	ucidef_set_led_netdev "wlan2g" "2.4G" "green:wlan-2ghz" "phy0-ap0"
	ucidef_set_led_netdev "wlan5g" "5G" "green:wlan-5ghz" "phy1-ap0"
	;;
abt,asr3000)
	ucidef_set_led_netdev "wan" "WAN" "green:wan" "eth1"
	ucidef_set_led_netdev "wlan2g" "WLAN2G" "green:wlan-2ghz" "phy0-ap0"